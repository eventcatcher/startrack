	#include <aes.h>
	...
	#define AC_HELP          1025
	#define HELP_FILE_MAGIC  0x1993

	int  my_apid;            /* my own id from appl_init */
	char helpname[] = "1STGUIDE";

	int send_help( char *keyword, char *indexfile )
	{
