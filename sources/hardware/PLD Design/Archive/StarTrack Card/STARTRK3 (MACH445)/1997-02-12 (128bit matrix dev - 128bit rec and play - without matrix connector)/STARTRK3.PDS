; ---------------------Record

ZR0.T = VCC;
ZR0.CLKF = /CLKREC;
ZR0.RSTF = RRESET;
ZR0.SETF = GND;

ZR1.T = ZR0;
ZR1.CLKF = /CLKREC;
ZR1.RSTF = RRESET;
ZR1.SETF = GND;

ZR2.T = ZR0 * ZR1;
ZR2.CLKF = /CLKREC;
ZR2.RSTF = RRESET;
ZR2.SETF = GND;

WR0.T = ZR0 * ZR1 * ZR2;
WR0.CLKF = /CLKREC;
WR0.RSTF = RRESET;
WR0.SETF = GND;

WR1.T = ZR0 * ZR1 * ZR2 * WR0;
WR1.CLKF = /CLKREC;
WR1.RSTF = RRESET;
WR1.SETF = GND;

WR2.T = ZR0 * ZR1 * ZR2 * WR0 * WR1;
WR2.CLKF = /CLKREC;
WR2.RSTF = RRESET;
WR2.SETF = GND;

WR3.T = ZR0 * ZR1 * ZR2 * WR0 * WR1 * WR2;
WR3.CLKF = /CLKREC;
WR3.RSTF = RRESET;
WR3.SETF = GND;

FSREC = AESSYNCI * /SWAES * /RSWM128
      + AESSYNCI * /SWAES * /RSWM128
      + ADLR * SWAES * /RSWM128
      + ADLR * SWAES * /RSWM128
      + DSPSC2 * RSWM128;

