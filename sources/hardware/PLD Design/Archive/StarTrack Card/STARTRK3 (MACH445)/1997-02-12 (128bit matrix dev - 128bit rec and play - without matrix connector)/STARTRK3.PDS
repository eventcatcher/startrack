RRESET = VCC;
RRESET.CLKF = FSREC;
RRESET.RSTF = RRCLR;

SHD0 = INPSDATA * /SWONDSP * /RSWM128
     + INPSDATA * SWINDSP * /RSWM128
     + DSPSTD * SWONDSP * /SWINDSP * /RSWM128
     + DSPSTD * RSWM128;
SHD0.CLKF = /CLKREC;
SHD0.SETF = GND;
SHD0.RSTF = GND;

