      + RP1 * ZP2 * ZP1 * /ZP0 * PSWM128
      + RP0 * ZP2 * ZP1 * ZP0  * PSWM128
      + /WSTART * WRESET;

DA_SDATA = ADATAI * /WR1 * SWAES * DA1 * /DA0
         + DAHLP * /DA1 * DA0
         + DSPSTD * DA1 * DA0 * DSPIN0
         + DSPSTD * /WR1 * DA1 * DA0 * DSPIN1 * /DSPIN0;

