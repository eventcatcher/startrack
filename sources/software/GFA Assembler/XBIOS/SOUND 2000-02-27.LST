               move.l     fifo,d2       ; fifolen  (len/2/4-1) bei 8192Byte=$3FF d2
               movea.l    vmestart,a0
               movea.l    a0,a1
               adda.l     #$40,a0       ; StatH im Bytemode
               adda.l     #$100,a1      ; Playread im Bytemode
