               bsr        matinsel
               move.w     #2,d0
               bsr        matoutsel     ; zu DAC-auswahl
               move.w     #2,d0         ; mit A/D verbinden
               move.w     d0,dacrec
               bsr        matinsel
               move.w     #3,d0
               bsr        matoutsel     ; zu DSPIN-auswahl
               move.w     #2,d0         ; mit A/D verbinden
               move.w     d0,dsprec
               move.w     d0,cardrec
