iendplay1c:    cmpi.l     #35,pimode
               bne.s      iendplay2
               bsr        startupmex
iendplay2:     movem.l    (sp)+,d0-d7/a0-a6
               rte        
iplooptst:     btst       #0,d3
               beq.s      iendplay
               lea.l      von,a4
               lea.l      bvon,a3
               move.l     (a3),(a4)     ; neue Anfangsposition
               bra        ip_back2

imempeakp:     movea.l    von,a2        ; von a2
