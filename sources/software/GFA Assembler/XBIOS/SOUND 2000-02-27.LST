corrkey2:      lea.l      pipefind,a5   ; midilist-location(midicount)
               clr.w      d7
               move.b     0(a5,d1.w),d7 ; midilisten-location holen
               add.w      d7,d7
               lea.l      midipipe,a5
               move.w     #-1,0(a5,d7.w)          ; entry aus midipipe entfernen
