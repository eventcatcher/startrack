               bclr       #0,d0         ; play-INT-l”schen
               move.b     d0,(a1)
               movea.l    vmestart(pc),a1
               move.b     d0,$280(a1)
               lea.l      pbufstrt(pc),a1
               move.l     (a1),(a5)     ; bufpointer auf Anfangswert setzen
