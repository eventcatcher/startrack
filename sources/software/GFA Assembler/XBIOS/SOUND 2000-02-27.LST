               lea.l      dsppath2(pc),a1
               move.b     (a1),d7
               bclr       #1,d7         ; Matrixin->MatrixOut
               bset       #2,d7
               bset       #3,d7         ; convertmode an
               move.b     d7,(a1)       ; und sichern
               lea.l      vmestart(pc),a6
               movea.l    (a6),a6
               move.b     d7,$214(a6)   ; und setzen
               move.w     #2,d0
               bsr        matoutsel     ; zu DAC-auswahl
