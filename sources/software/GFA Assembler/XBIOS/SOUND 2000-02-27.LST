; DSP2----------------------DSP-Supportroutinen-------------------
;
;L„nge der LOD-Datei ber Fseek ermitteln (keine DTA notwendig),
;Puffer allozieren und Datei laden
;
;Eingaben
;        a0:     Zeiger auf Dateiname
;
;Ausgabe
;        d0:     Pufferadr oder 0L
;        a0:     LOD-Endadresse
;
read_LODfile:  
               movem.l    d5-d7,-(sp)   ;sichern

               clr.w      -(sp)         ;READ_ONLY
               move.l     a0,-(sp)      ;Zeiger auf den Dateinamen
               move.w     #$3d,-(sp)    ;Fopen
               trap       #GEMDOS
               addq.l     #8,sp
               move.l     d0,d7         ;handle
               bpl.s      get_LOD_len   ;Datei vorhanden
               moveq.l    #0,d0         ;Fehler aufgetreten
               bra.s      exit_read_LODfile

get_LOD_len:   
               move.w     #2,-(sp)      ;ans Dateiende
               move.w     d7,-(sp)      ;handle
               clr.l      -(sp)         ;offset
               move.w     #$42,-(sp)    ;Fseek
               trap       #GEMDOS
               lea.l      10(sp),sp
               move.l     d0,d6         ;Dateil„nge
               bgt.s      seek_LOD_start          ;Datei > 0 Bytes
               moveq.l    #0,d0         ;Fehler aufgetreten
               bra.s      exit_read_LODfile

seek_LOD_start:           
               clr.w      -(sp)         ;an Dateianfang
               move.w     d7,-(sp)      ;handle
               clr.l      -(sp)         ;offset
               move.w     #$42,-(sp)    ;Fseek
               trap       #GEMDOS
               lea.l      10(sp),sp

               move.l     d6,-(sp)      ;Dateil„nge
               move.w     #$48,-(sp)    ;Malloc
               trap       #GEMDOS
               addq.l     #6,sp
               move.l     d0,d5         ;Pufferadresse
               bne.s      _read_LOD
               moveq.l    #0,d0         ;Fehler aufgetreten
               bra.s      exit_read_LODfile

_read_LOD:     
               move.l     d5,-(sp)      ;Puffer
               move.l     d6,-(sp)      ;count
               move.w     d7,-(sp)      ;handle
               move.w     #$3f,-(sp)    ;Fread
               trap       #GEMDOS
               lea.l      12(sp),sp
               cmp.l      d0,d6         ;alle Bytes gelesen?
               beq.s      _close_LOD
               moveq.l    #0,d0         ;Fehler aufgetreten
               bra.s      exit_read_LODfile

_close_LOD:    
               move.w     d7,-(sp)      ;handle
               move.w     #$3e,-(sp)    ;Fclose
               trap       #GEMDOS
               addq.l     #4,sp

               movea.l    d5,a0         ;Pufferadr
               adda.l     d6,a0         ;Puffer + L„nge = _dsp_LOD_endadr

               move.l     d5,d0         ;Pufferadr != 0: alles ok
exit_read_LODfile:        
               movem.l    (sp)+,d5-d7   ;Register zurck
               rts        

