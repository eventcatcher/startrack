               move.w     #2,d0         ; mit quelle ADC verbinden
               move.w     d0,(a0)       ; und sichern
               bsr        matinsel      ; Verbindung zu ADC setzen
adout1:        lea.l      dsprec(pc),a0 ; quelle von DSPREC(ziel) holen
