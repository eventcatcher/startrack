               dbra       d0,cdig0
               move.l     $4ba,d1       ; Starttime
cdig1:         move.b     (a0),d0       ; Flag lesen
               move.l     $4ba,d4       ; Endtime
               sub.l      d1,d4         ; time
               cmpi.l     #50,d4
               bgt.s      cdigend
               btst       #1,d0         ; RECORD-FIFO voll ?
               bne.s      cdig1
               move.l     d2,d0         ; fifodepth/2-1
               subq.l     #1,d0
               add.l      d2,d6
